{
  "name": "react-parcel",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "parcel index.html",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "cypress": "start-server-and-test start http://localhost:1234 cypress:run",
    "format": "prettier --write \"**/*.{js,css,md}\""
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "cypress": "^3.4.1",
    "husky": ">=1",
    "less": "^3.9.0",
    "lint-staged": ">=8",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "start-server-and-test": "^1.10.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
